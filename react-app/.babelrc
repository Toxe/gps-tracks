{
    "presets": [
        "react-app"
    ],
    "plugins": [
        [
            "i18next-extract",
            {
                "locales": [
                    "en",
                    "de"
                ],
                "outputPath": "public/locales/{{locale}}/{{ns}}.json",
                "discardOldKeys": true,
                "jsonSpace": 4
            }
        ]
    ]
}